    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "uuid": "^8.3.2"
  }
